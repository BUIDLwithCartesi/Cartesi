{
  "name": "cartesi",
  "displayName": "Cartesi",
  "description": "A VS Code extension to streamline Cartesi Rollups and Machines development.",
  "version": "0.1.0",
  "publisher": "",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages",
    "Debuggers",
    "Other"
  ],
  "activationEvents": [
    "onCommand:cartesi.runEmulator",
    "onCommand:cartesi.showDocumentation",
    "onView:cartesiView"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cartesi.runEmulator",
        "title": "Run Cartesi Emulator"
      },
      {
        "command": "cartesi.showDocumentation",
        "title": "Show Cartesi Documentation"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "cartesi",
          "title": "Cartesi",
          "icon": "images/cartesi-icon.png"
        }
      ]
    },
    "views": {
      "cartesi": [
        {
          "id": "cartesiView",
          "name": "Cartesi View"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "vsce package"
  },
  "dependencies": {}
}