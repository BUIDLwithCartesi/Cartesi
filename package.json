{
    "name": "cartesi",
    "displayName": "Cartesi",
    "publisher": "Cartesi",
    "version": "0.0.1",
    "description": "A Visual Studio Code extension for Cartesi.",
    "repository": {
      "type": "git",
      "url": "https://github.com/your-username/cartesi-gateway.git"
    },
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": ["Other"],
    "activationEvents": ["onCommand:cartesi.runEmulator"],
    "main": "./extension.js",
    "contributes": {
      "commands": [
        {
          "command": "cartesi.runEmulator",
          "title": "Run Cartesi Emulator"
        },
        {
          "command": "cartesi.showDocumentation",
          "title": "Show Cartesi Documentation"
        }
      ]
    },
    "scripts": {
      "package": "vsce package"
    },
    "dependencies": {}
  }
  